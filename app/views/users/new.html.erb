 <%= form_for(@user, url: user_new_path) do |f| %>
 
  <div class="field">
    <%= f.label :first_name %><br />
    <%= f.text_field :first_name%>
  </div>

  <div class="field">
    <%= f.label :last_name %><br />
    <%= f.text_field :last_name %>
  </div>

  <div class="field">
    <%= f.label :phone_number%><br />
    <%= f.number_field :phone_number %>
  </div>

  <div class="actions">
    <%= f.submit "Save" %>
  </div>
<% end %>